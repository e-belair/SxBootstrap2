{
    "name" : "manuscle/sxbootstrap2",
    "description" : "A twitter bootstrap module for ZF2 (zend framework 2) based on rwoverdijk/assetmanager",
    "license" : "BSD-2-Clause",
    "keywords" : [
        "twitter",
        "bootstrap",
        "assetmanager",
        "zf2",
        "zend framework"
    ],
    "homepage" : "https://github.com/manuscle/SxBootstrap2",
    "require" : {
        "php" : ">=5.3.3",
        "spoonx/sxcore" : "1.*"
    },
    "require-dev" : {
        "zendframework/zendframework": "dev-master",
        "spiffy/spiffy-navigation" : "dev-master",
        "phpunit/phpunit" : "*"
    },
    "replace" : {
        "rwoverdijk/sxbootstrap" : "*"
    },
    "authors" : [
        {
            "name" : "Wesley Overdijk",
            "email" : "r.w.overdijk@gmail.com",
            "homepage" : "http://blog.spoonx.nl/"
        }
    ],
    "autoload" : {
        "psr-0" : {
            "SxBootstrap" : "src/"
        },
        "classmap" : [
            "./"
        ]
    },
    "suggest" : {
        "spiffy/spiffy-navigation" : "Required for SxNavigationMenu and dropdown",
        "rwoverdijk/assetmanager" : "Required for the renderer",
        "twbs/bootstrap" : "Required for the renderer",
        "fortawesome/font-awesome" : "Required for Font Awesome integration"
    }
}
